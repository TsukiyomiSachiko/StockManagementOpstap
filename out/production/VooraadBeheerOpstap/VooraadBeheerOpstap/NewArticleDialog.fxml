<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VooraadBeheerOpstap.NewArticleDialog">
   <children>
      <ButtonBar layoutX="407.0" layoutY="346.0" prefHeight="40.0" prefWidth="179.0">
        <buttons>
          <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonPressed" text="OK" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonPressed" text="Cancel" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="articleName" layoutX="134.0" layoutY="14.0" prefHeight="25.0" prefWidth="333.0" promptText="Artikel naam" />
      <CheckBox fx:id="isLendArticle" layoutX="245.0" layoutY="50.0" mnemonicParsing="false" text="Is uitleen artikel?" />
      <CheckBox fx:id="isStockArticle" layoutX="242.0" layoutY="77.0" mnemonicParsing="false" onAction="#isStockArticleToggle" text="Is vooraad artikel?" />
      <TextField fx:id="minimumStock" layoutX="226.0" layoutY="107.0" promptText="Minimum vooraad" visible="false" />
   </children>
</AnchorPane>
